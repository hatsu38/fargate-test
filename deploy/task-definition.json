{
  "executionRoleArn": "arn:aws:iam::318288222771:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "fargate-test-rails",
      "logConfiguration": {
        "logDriver": "awslogs",
        "secretOptions": null,
        "options": {
          "awslogs-group": "/ecs/fargate-test",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs/fargate-test"
        }
      },
      "entryPoint": [],
      "portMappings": [
        {
          "hostPort": 3000,
          "protocol": "tcp",
          "containerPort": 3000
        }
      ],
      "command": [
        "/app/entrypoint.sh"
      ],
      "cpu": 0,
      "healthCheck": {
        "retries": 10,
        "command": [
          "CMD-SHELL",
          "curl localhost:3000",
          "\"|| exit 1\""
        ],
        "timeout": 30,
        "interval": 5,
        "startPeriod": 30
      },
      "environment": [
        {
          "name": "RAILS_ENV",
          "value": "production"
        },
        {
          "name": "RAILS_LOG_TO_STDOUT",
          "value": "true"
        },
        {
          "name": "RAILS_SERVE_STATIC_FILES",
          "value": "true"
        }
      ],
      "secrets": [
        {
          "valueFrom": "/fargate-test/db-host",
          "name": "DB_HOST"
        },
        {
          "valueFrom": "/fargate-test/db-password",
          "name": "DB_PASSWORD"
        },
        {
          "valueFrom": "/fargate-test/db-username",
          "name": "DB_USERNAME"
        },
        {
          "valueFrom": "/fargate-test/rails-master-key",
          "name": "RAILS_MASTER_KEY"
        },
        {
          "valueFrom": "/fargate-test/redis-url",
          "name": "REDIS_URL"
        }
      ]
    },
    {
      "name": "fargate-test-nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/fargate-test",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs/fargate-test"
        }
      },
      "dependsOn": [
        {
          "containerName": "fargate-test-rails",
          "condition": "HEALTHY"
        }
      ],
      "volumesFrom": [
        {
          "sourceContainer": "fargate-test-rails",
          "readOnly": true
        }
      ]
    }
  ],
  "placementConstraints": [],
  "memory": "512",
  "taskRoleArn": "arn:aws:iam::318288222771:role/ecsTaskExecutionRole",
  "compatibilities": [
    "FARGATE"
  ],
  "family": "fargate-test-app",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "revision": 5,
  "status": "ACTIVE",
  "volumes": []
}